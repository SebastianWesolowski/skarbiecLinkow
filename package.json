{
  "name": "s-next",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:info:start": "Build------------------------------------------------------------------------------------------",
    "copy:assets": "cpy './src/assets/**/*' '!**.tsx' '!**.gitkeep' './public/assets' --parents",
    "build": "run-s clean copy:assets build:next",
    "postbuild": "next-sitemap",
    "build:prod": "run-s clean copy:assets build:next next:export postbuild",
    "build:next": "next build",
    "clean": "rimraf .next out public/assets public/sitemap*",
    "analyze": "cross-env ANALYZE=true yarn build",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "next:export": "next export",
    "build:info:end": "Build-End----------------------------------------------------------------------------------------",
    "lint:info:start": "Lint-and-Check----------------------------------------------------------------------------------",
    "lint": "run-s  format:check lint:parallel",
    "lint:parallel": "run-p lint:check style:check:strict",
    "lint:check": "eslint \"./src/**/*.+(js|jsx|ts|tsx)\"",
    "lint:check:strict": "cross-env NODE_ENV=strictRules npm run lint:check -- --report-unused-disable-directives",
    "lint:fix": "run-s format:fix \"lint:check -- --fix\" style:fix",
    "lint:fix:strict": "cross-env NODE_ENV=strictRules npm run lint:fix -- --report-unused-disable-directives",
    "style:check": "stylelint \"./src/**/*.+(css|scss|sass)\"",
    "style:check:strict": "cross-env NODE_ENV=strictRules npm run style:check --",
    "style:fix": "run-s \"style:check --fix\"",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "npm run format:check -- --write",
    "format:check": "prettier --check \"./**/*.+(js|jsx|ts|tsx|json)\" --ignore-unknown --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{ts,js,tsx,md}\"",
    "lint:info:end": "Lint-End------------------------------------------------------------------------------------------",
    "Development:info:start": "Development------------------------------------------------------------------------------",
    "dev": "cross-env FORCE_COLOR=1 next dev",
    "start": "next start",
    "start:dev": "yarn run dev",
    "postinstall": "npx patch-package -y",
    "preinstall": "npx only-allow yarn",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg",
    "Development:info:end": "Development-End----------------------------------------------------------------------------",
    "Test:info:start": "Test--------------------------------------------------------------------------------------------",
    "test": "jest -maxWorkers=25% --passWithNoTests",
    "test:ci": "jest --ci",
    "test:watch": "jest",
    "test:coverage": "run-s \"test -- --coverage\" test:clear",
    "test:clear": "rimraf ./reports/coverage && mv coverage ./reports/",
    "test:update": "jest --updateSnapshot",
    "test:changed": "jest --onlyChanged",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "Test:info:end": "Test-End------------------------------------------------------------------------------------------",
    "GitHook:info:start": "GitHook--------------------------------------------------------------------------------------",
    "husky:prepare-commit-msg": "exec < /dev/tty && node_modules/.bin/git-cz --hook || true",
    "husky:pre-commit": "lint-staged -c ./.husky/lint-staged.config.json",
    "husky:pre-push": "run-s lint quality:check:* build:prod",
    "husky:commit-msg": "commitlint --edit $GIT_PARAMS",
    "quality:check:type": "echo quality:check:type",
    "staged:style:check": "stylelint \"*.+(css|scss|sass)\" --allow-empty-input",
    "staged:style:fix": "yarn style:check --fix",
    "staged:format:check": "npm run format:check",
    "staged:format:fix": "yarn format:fix",
    "staged:lint:check": "eslint --config .eslintrc.js",
    "staged:lint:fix": "yarn lint:check --fix",
    "prepare": "husky install",
    "GitHook:info:end": "GitHook-End----------------------------------------------------------------------------"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.13",
    "@fontsource/lexend": "^5.0.15",
    "@next/bundle-analyzer": "^13.3.0",
    "@radix-ui/react-accordion": "^1.1.1",
